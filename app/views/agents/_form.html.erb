
<%= form_for @agent , url: {action: "create"} do |f| %>  
<div class="form-group row">
<label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :firstname %></label>
    <%= f.text_field :first_name %>
  </div>
  </div>

  <div class="form-group row">
  <label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :lastname %></label>
    <%= f.text_field :last_name %>
  </div>
  </div>
  </div>
  </div><!--end of form-group-->
 </div><!--end of row-->
 <div class="form-group row">
 <label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :address %></label>
    <%= f.number_field :address %>
  </div>
 <div class="form-group row">
 <label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :mobile_number %></label>
    <%= f.number_field :mobile_number %>
  </div>
   </div>
   <div class="form-group row">
   <label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :id_proof%></label>
    <%= f.text_field :id_proof %>
  </div>
  </div>
  <div class="form-group row">
  <label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :agency_name %></label>
    <%= f.text_field :agency_name %>
  </div>
  </div>
<div class="form-group row">
<label for="example-text-input" class="col-xs-3 col-form-label">
  
    <%= f.label :agency_phone %></label>
    <%= f.text_field :agency_phone %>
  </div>
  </div>
  <div class="form-group">
        <%= f.submit "Submit", class: "btn btn-primary" %>

    
    </div>


</div>
</div>
<% end %>


